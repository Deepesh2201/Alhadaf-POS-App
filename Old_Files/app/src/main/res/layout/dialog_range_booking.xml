<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ln_dlg_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_dialog_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp5dp"
        android:layout_marginRight="@dimen/dp5dp"
        android:background="@android:color/transparent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="85dp"
            android:background="@drawable/bg_border_dark"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp10dp"
                android:layout_marginTop="@dimen/dp50dp"
                android:layout_marginEnd="@dimen/dp10dp"
                android:layout_marginBottom="@dimen/dp5dp"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_lbl_select_range"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp5dp"
                        android:text="Select Range"
                        android:textColor="@color/secondaryTextColor" />

                    <EditText
                        android:id="@+id/edt_sel_range"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp45dp"
                        android:layout_margin="@dimen/dp5dp"
                        android:layout_weight="1"
                        android:background="@drawable/bg_border_whitebg"
                        android:clickable="false"
                        android:cursorVisible="false"
                        android:drawableRight="@drawable/ic_drop_down_fill_24"
                        android:drawableTint="@color/colorPrimaryDark"
                        android:elevation="@dimen/dp1dp"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:gravity="center"
                        android:hint="Select Range"
                        android:paddingEnd="@dimen/dp10dp"
                        android:textColor="@color/primaryTextColor"
                        android:textSize="@dimen/dp16dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_lbl_select_lane"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp5dp"
                        android:text="Select Lane"
                        android:textColor="@color/secondaryTextColor" />


                    <EditText
                        android:id="@+id/edt_sel_lane"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp45dp"
                        android:layout_margin="@dimen/dp5dp"
                        android:layout_weight="1"
                        android:background="@drawable/bg_border_whitebg"
                        android:clickable="false"
                        android:cursorVisible="false"
                        android:drawableRight="@drawable/ic_drop_down_fill_24"
                        android:drawableTint="@color/colorPrimaryDark"
                        android:elevation="@dimen/dp1dp"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:gravity="center"
                        android:hint="Select Lane"
                        android:paddingEnd="@dimen/dp10dp"
                        android:textColor="@color/primaryTextColor"
                        android:textSize="@dimen/dp16dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ln_cancel_dlg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dp5dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_lbl_cancel"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/button_height"
                            android:background="@color/colorPrimaryDark"
                            android:gravity="center"
                            android:text="Cancel"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp16dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ln_book_range"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dp5dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_btn_book_range"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/button_height"
                            android:background="@color/colorPrimaryDark"
                            android:gravity="center"
                            android:text="Book"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp16dp" />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

        <FrameLayout
            android:id="@+id/logolin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/dp40dp"
            android:background="@android:color/transparent"
            android:gravity="center">


            <ImageView
                android:id="@+id/iv_gif"
                android:layout_width="@dimen/dp120dp"
                android:layout_height="@dimen/dp100dp"
                android:layout_gravity="center"
                android:src="@drawable/loader"
                android:visibility="gone" />

            <ImageView
                android:layout_width="@dimen/dp100dp"
                android:layout_height="@dimen/dp90dp"
                android:layout_gravity="center"
                android:background="@drawable/bg_border_dark"
                android:padding="@dimen/dp10dp"
                android:src="@drawable/app_logo_icon" />


        </FrameLayout>

    </RelativeLayout>


</LinearLayout>