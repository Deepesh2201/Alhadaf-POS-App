<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:maxLevel="0">
        <shape>
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/button_corner" />
        </shape>
    </item>

    <item android:maxLevel="1">
        <shape>
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="@color/white" />
            <corners android:radius="@dimen/button_corner" />
        </shape>
    </item>

    <item android:maxLevel="2">
        <shape>

            <solid android:color="@color/main_blue" />

            <corners android:radius="5dp" />
        </shape>
    </item>


    <item android:maxLevel="5">
        <shape>
            <solid android:color="@color/main_blue" />
            <stroke
                android:width="1dp"
                android:color="@color/main_blue" />
            <corners
                android:bottomLeftRadius="@dimen/button_corner"
                android:bottomRightRadius="@dimen/button_corner" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
    <item android:maxLevel="6">
        <shape>
            <solid android:color="@color/main_blue" />
            <stroke
                android:width="1dp"
                android:color="@color/main_blue" />
            <corners android:radius="@dimen/button_corner" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
    <item android:maxLevel="7">
        <shape>
            <solid android:color="@color/Gray" />
            <stroke
                android:width="2dp"
                android:color="@color/Gray" />
            <corners android:radius="@dimen/button_corner" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
    <item android:maxLevel="8">
        <shape>
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="@color/white" />
            <corners
                android:topLeftRadius="@dimen/button_corner"
                android:topRightRadius="@dimen/button_corner" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>

    <item android:maxLevel="9">
        <shape>
            <solid android:color="@color/Gray_dark" />
            <stroke
                android:width="2dp"
                android:color="@color/Gray_dark" />
            <corners
                android:radius="@dimen/button_corner"/>
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
    <item android:maxLevel="10">
        <shape>
            <solid android:color="@color/main_blue" />
            <stroke
                android:width="2dp"
                android:color="@color/main_blue" />
            <corners
                android:bottomLeftRadius="@dimen/button_corner"
